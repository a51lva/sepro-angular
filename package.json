{
  "name": "sepro-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "tsc": "tsc -p tsconfig.json",
    "rollup": "rollup -f iife dist/src/main.js -o dist/src/bundle.es2015.js",
    "es5": "tsc --target es2018 --allowJs dist/src/bundle.es2015.js --out dist/src/bundle.js",
    "minify": "terser dist/src/bundle.js --screw-ie8 --compress --mangle --output dist/src/bundle.min.js",
    "build_prod": "npm-run-all tsc rollup es5 minify",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~7.2.10",
    "@angular/compiler": "~7.2.10",
    "@angular/core": "~7.2.10",
    "@angular/forms": "~7.2.10",
    "@angular/platform-browser": "~7.2.10",
    "@angular/platform-browser-dynamic": "~7.2.10",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~7.2.10",
    "@angular/service-worker": "~7.2.0",
    "@auth0/angular-jwt": "github:auth0/angular2-jwt",
    "@types/googlemaps": "^3.30.19",
    "@types/source-map": "^0.5.2",
    "auth0-js": "^9.10.0",
    "base-64": "^0.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.10",
    "@angular/language-service": "~7.2.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "browserify": "^16.2.3",
    "codelyzer": "~4.5.0",
    "google-maps-mock": "github:dicbrus/google-maps-mock",
    "hammerjs": "^2.0.8",
    "i": "^0.3.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rollup": "^1.7.4",
    "rollup-plugin-node-resolve": "^4.0.1",
    "tar": "^4.4.8",
    "terser": "^3.17.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "uglify-js": "^3.5.2"
  },
  "architect": {
    "serve": {
      "builder": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "sepro-angular:build",
        "proxyConfig": "proxy.conf.json"
      }
    }
  }
}
